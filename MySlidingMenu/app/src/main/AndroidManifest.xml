<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myslidingmenu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Light.NoTitleBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ViewDragSlideMenuActivity"/>
        <activity android:name=".ViewGroupSlideMenuActivity"/>
        <activity android:name=".DrawerLayoutActivity"/>
        <activity android:name=".LeftAndRightDrawerActivity"/>
        <activity android:name=".qq.QQLeftAndRightDrawerActivity"/>
    </application>

</manifest>